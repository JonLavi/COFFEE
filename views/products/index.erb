<h1>All Products</h1>

<table>
  <tr>
    <th>Id</th>
    <th>|</th>
    <th>Name</th>
    <th>|</th>
    <th>Producer</th>
  </tr>
  <% @products.each do |product| %>
  <tr>
    <td><a href="/products/<%= product.id %>"><%= product.id %></a></td>
    <td>|</td>
    <td><%= product.name %></td>
    <td>|</td>
    <td><a href="/producers/<%= product.producer_id %>"><%= product.producer_id %></a></td>
  </tr>
  <% end %>
</table>
